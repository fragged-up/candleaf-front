    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.5.2",
    "prettier": "^3.5.0",
    "tailwindcss": "^4.0.6",
    "typescript": "~5.7.3",
